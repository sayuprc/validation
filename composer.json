{
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.7",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
      "psr-4": {
        "Validation\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": [
        "phpunit"
      ],
      "analyze": [
        "phpstan analyze"
      ]
    }
}
